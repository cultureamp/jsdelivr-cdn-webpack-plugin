{
  "name": "jsdelivr-cdn-webpack-plugin",
  "version": "1.0.0",
  "description": "Dynamically get your dependencies from JSDelivr cdn rather than bundling them in your app",
  "license": "MIT",
  "repository": "sparkboom/jsdelivr-cdn-webpack-plugin",
  "author": {
    "name": "Matthew McLeod",
    "email": "matt@sparkboom.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "eslint ./src && nyc ava",
    "prepublishOnly": "npm run build",
    "build": "babel src --out-dir lib"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "keywords": [
    "webpack",
    "cdn",
    "unpkg.com",
    "html-webpack-plugin",
    "webpack-manifest-plugin",
    "html",
    "script",
    "assets",
    "externals"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "read-pkg-up": "^4.0.0",
    "resolve-pkg": "^1.0.0"
  },
  "peerDependencies": {
    "webpack": "4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-rc.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.4",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mz": "^2.6.0",
    "nyc": "^12.0.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-manifest-plugin": "2.0.0-rc.2"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": true
      }
    },
    "files": [
      "test/**/*.js",
      "!test/fixtures/output/**/*"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
